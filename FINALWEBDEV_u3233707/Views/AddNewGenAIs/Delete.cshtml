@model FINALWEBDEV_u3233707.Models.AddNewGenAI



@{
    ViewData["Title"] = "Delete";
}
<div class="container-fluid background1">
    <h1 id="heading1">Delete GenAI Website</h1>
    <div class="container yellowcontainer">
        <h3 class="yellowheading">Are you sure you want to delete this website?</h3>
    </div>
    
    <div class="bottompadding">

       
        <hr />
        <dl class="row">
            <dt class = "col-sm-2">
                <p class="text-white">@Html.DisplayNameFor(model => model.GenAIName)</p>
            </dt>
            <dd class = "col-sm-10">
                <p class="text-white">@Html.DisplayFor(model => model.GenAIName)</p>
            </dd>
            <dt class = "col-sm-2">
                <p class="text-white">@Html.DisplayNameFor(model => model.Summary)</p>
            </dt>
            <dd class = "col-sm-10">
                <p class="text-white">@Html.DisplayFor(model => model.Summary)</p>
            </dd>
            <dt class="col-sm-2">
                <p class="text-white">Image FileName</p>
            </dt>
            <dd class="col-sm-10">
                <p class="text-white" id="replacement">@Html.DisplayFor(model => model.AnchorLink)</p>
            </dd>
            <dt class="col-sm-2">
                <p class="text-white">@Html.DisplayNameFor(model => model.AnchorLink)</p>
            </dt>
            <dd class="col-sm-10">
                <p class="text-white">@Html.DisplayFor(model => model.AnchorLink)</p>
            </dd>
            <dt class="col-sm-2">
                <p class="text-white">@Html.DisplayNameFor(model => model.Like)</p>
            </dt>
            <dd class="col-sm-10">
                <p class="text-white">@Html.DisplayFor(model => model.Like)</p>
            </dd>
        </dl>

        <script>

            var replacement = document.getElementById("replacement");
            var newtext = replacement.innerHTML;
            var newreplacement = newtext.replace("/images/", "");
            replacement.innerHTML = newreplacement;


        </script>
    
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="Index" class="text-white-50">Back to List</a>
        </form>
    </div>

</div>
